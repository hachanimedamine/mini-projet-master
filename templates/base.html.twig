<!DOCTYPE html>
<html>
<head>
	<meta charset="UTF-8">
	<title>{% block title %}BIENVENU!{% endblock %}</title>
	{# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
	{% block stylesheets %}
		<!-- Site Icons -->
		<link rel="shortcut icon" href="{{asset('images/favicon.ico')}}" type="image/x-icon">
		<link rel="apple-touch-icon" href="{{asset('images/apple-touch-icon.png')}}">

		<!-- Bootstrap CSS -->
		<link rel="stylesheet" href="{{asset('css/bootstrap.min.css')}}">
		<!-- Site CSS -->
		<link rel="stylesheet" href="{{asset('css/style.css')}}">
		<!-- Responsive CSS -->
		<link rel="stylesheet" href="{{asset('css/responsive.css')}}">
		<!-- Custom CSS -->
		<link rel="stylesheet" href="{{asset('css/custom.css')}}">

		{#{{ encore_entry_link_tags('app') }}#}
	{% endblock %}

	{% block javascripts %}
		{#{{ encore_entry_script_tags('app') }}#}

		<!-- ALL JS FILES -->
		<script src="{{asset('js/jquery-3.2.1.min.js')}}"></script>
		<script src="{{asset('js/popper.min.js')}}"></script>
		<script src="{{asset('js/bootstrap.min.js')}}"></script>
		<!-- ALL PLUGINS -->
		<script src="{{asset('js/jquery.superslides.min.js')}}"></script>
		<script src="{{asset('js/images-loded.min.js')}}"></script>
		<script src="{{asset('js/isotope.min.js')}}"></script>
		<script src="{{asset('js/baguetteBox.min.js')}}"></script>
		<script src="{{asset('js/form-validator.min.js')}}"></script>
		<script src="{{asset('js/contact-form-script.js')}}"></script>
		<script src="{{asset('js/custom.js')}}"></script>
	{% endblock %}
</head>
<body>

{% block header %}
	<!-- Start header -->
	{% if app.user %}

		<header class="top-navbar">
			<nav class="navbar navbar-expand-lg navbar-light bg-light">
				<div class="container">
					<a class="navbar-brand" href="{{ path('home') }}">
						<img src="images/wakalni.png" alt="" />
					</a>
					<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbars-rs-food" aria-controls="navbars-rs-food" aria-expanded="false" aria-label="Toggle navigation">
						<span class="navbar-toggler-icon"></span>
					</button>
					<div class="collapse navbar-collapse" id="navbars-rs-food">
						<ul class="navbar-nav ml-auto">
							<li class="nav-item"><a class="nav-link" href="{{ path('home') }}">Home</a></li>
							<li class="nav-item"><a class="nav-link" href="{{ path('categorie_index') }}">Catégorie</a></li>
							<li class="nav-item"><a class="nav-link" href="{{ path('plat_index') }}">Plat</a></li>
							<li class="nav-item"><a class="nav-link" href="{{ path('panier') }}">Panier</a></li>
							<li class="nav-item"><a class="nav-link" href="">{{ app.user.username }} </a></li>
							<li class="nav-item"><a class="nav-link" href="{{ path('app_logout') }}"> Logout</a></li>
						</ul>
					</div>
				</div>
			</nav>
		</header>
	{% else %}

	<header class="top-navbar">
		<nav class="navbar navbar-expand-lg navbar-light bg-light">
			<div class="container">
				<a class="navbar-brand" href="index.html">
					<img src="images/wakalni.png" alt="" />
				</a>
				<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbars-rs-food" aria-controls="navbars-rs-food" aria-expanded="false" aria-label="Toggle navigation">
					<span class="navbar-toggler-icon"></span>
				</button>
				<div class="collapse navbar-collapse" id="navbars-rs-food">
					<ul class="navbar-nav ml-auto">
						<li class="nav-item"><a class="nav-link" href="{{ path('home') }}">Home</a></li>
						<li class="nav-item"><a class="nav-link" href="{{ path('categorie_index') }}">Catégorie</a></li>
						<li class="nav-item"><a class="nav-link" href="{{ path('plat_index') }}">Plat</a></li>
						<li class="nav-item"><a class="nav-link" href="{{ path('panier') }}">Panier</a></li>
						<li class="nav-item"><a class="nav-link" href="{{ path('app_login') }}">Sign In</a></li>
						<li class="nav-item"><a class="nav-link" href="{{ path('app_register') }}">Sign Up</a></li>
					</ul>
				</div>
			</div>
		</nav>
	</header>
	{% endif %}
	<!-- End header -->
{% endblock %}

{% block body %}{% endblock %}


{% block footer %}
	<!-- Start Footer -->
	<footer class="footer-area bg-f">
		<div class="container">
			<div class="row">

				<div class="col-lg-3 col-md-6">
					<h3>Disponibilité </h3>
					<p><span class="text-color">Lundi: </span> Fermé</p>
					<p><span class="text-color">Mardi-Mercredi :</span> 06:00 - 22:00</p>
					<p><span class="text-color">Jeudi-Vendredi :</span> 06:00 - 23:00</p>
					<p><span class="text-color">Samedi-Dimanche :</span> 05:00 - 00:00</p>
				</div>
				<div class="col-lg-3 col-md-6">
					<h3>Contact information</h3>
					<p class="lead">CITE OLIMPIQUE TUNIS</p>
					<p class="lead">+21699162141</p>
					<p><a href="#"> wakalnisupport@gmail.com</a></p>
				</div>
				<div class="col-lg-3 col-md-6">
					<h3>A PROPOS DE NOUS</h3>
					<p>Le restaurant connu sous le nom "WAKALNI" apparaît autour du XVIII siècle en France</p>
				</div>
			</div>
		</div>

		<div class="copyright">
			<div class="container">
				<div class="row">
					<div class="col-lg-12">
						<p class="company-name">All Rights Reserved. &copy; 2018 <a href="#"> RESTAURANT WAKKALNI</a> Design By : HWABS

					</div>
				</div>
			</div>
		</div>

	</footer>
	<!-- End Footer -->

	<a href="#" id="back-to-top" title="Back to top" style="display: none;">&uarr;</a>


{% endblock %}
</body>
</html>